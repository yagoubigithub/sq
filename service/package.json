{
  "name": "@sq-communicator/service",
  "version": "0.1.0",
  "description": "smart-Q Comunicator service",
  "homepage": "https://smart-q.de",
  "author": {
    "name": "smart-Q GmbH",
    "email": "info@smart-q.de"
  },
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "postinstall": "npm run build",
    "build": "tsc",
    "start": "ts-node src/index.ts",
    "test-install": "npm start -- --install",
    "test": "echo \"Error: no test specified\" && exit 1",
    "package:linux": "npm run build && pkg . --targets linux --output release/linux/sq-service",
    "package:windows": "npm run build && pkg . --targets win --output release/win/sq-service.exe",
    "package:macos": "npm run build && pkg . --targets macos --output release/macos/sq-service"
  },
  "bin": {
    "gdt-service": "dist/index.js"
  },
  "devDependencies": {
    "@types/md5": "^2.2.0",
    "@types/node": "^14.0.5",
    "@types/os-service": "^2.2.0",
    "@types/qs": "^6.9.3",
    "@types/socket.io": "^2.1.8",
    "pkg": "^5.8.1",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "@marsaud/smb2": "^0.18.0",
    "@sq-communicator/core": "file:../core",
    "axios": "^0.27.2",
    "chokidar": "^3.5.3",
    "os-service": "^2.2.0",
    "socket.io": "^2.3.0",
    "sqlite3": "^4.2.0",
    "sudo-prompt": "^9.2.1",
    "typeorm": "^0.2.25",
    "winston": "^3.3.3"
  }
}
